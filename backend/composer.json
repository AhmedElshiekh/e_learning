{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [ "framework", "laravel" ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "akaunting/setting": "^1.2.7",
        "bumbummen99/shoppingcart": "^2.9",
        "fideloper/proxy": "^4.4",
        "firebase/php-jwt": "^5.3",
        "fruitcake/laravel-cors": "^2.0",
        "fsuuaas/laravel-plupload": "^3.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "hesto/multi-auth": "^2.0",
        "jamesmills/laravel-timezone": "^1.9",
        "laravel/framework": "^8.40",
        "laravel/helpers": "^1.4",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^2.0",
        "paypal/rest-api-sdk-php": "*",
        "plank/laravel-mediable": "^5.1",
        "realrashid/sweet-alert": "^3.2",
        "spatie/laravel-permission": "^4.0",
        "spatie/laravel-translatable": "^4.6",
        "twilio/sdk": "^6.24",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "sbamtr/laravel-source-encrypter": "^1.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
